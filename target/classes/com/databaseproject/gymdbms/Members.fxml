<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="488.0" prefWidth="712.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.databaseproject.gymdbms.MembersController">
    <children>
        <Button layoutX="625.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToEmployees" prefHeight="28.0" text="Employees" />
        <Button layoutX="554.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToHome" prefHeight="28.0" prefWidth="62.0" text="Home" />
        <TableView layoutX="14.0" layoutY="81.0" prefHeight="352.0" prefWidth="329.0">
            <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
            </columns>
        </TableView>
        <Button layoutX="318.0" layoutY="443.0" mnemonicParsing="false" onAction="#addMember" text="+" />
        <Button layoutX="285.0" layoutY="443.0" mnemonicParsing="false" onAction="#deleteMember" prefHeight="25.0" prefWidth="25.0" text="-" />
        <Label layoutX="14.0" layoutY="13.0" text="Members Page">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <TextField alignment="CENTER" layoutX="213.0" layoutY="49.0" onAction="#searchForMember" prefHeight="25.0" prefWidth="130.0" text="Search by name" />
        <BarChart layoutX="369.0" layoutY="81.0" prefHeight="352.0" prefWidth="329.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </BarChart>
    </children>
</AnchorPane>
